<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>

    <link href="~/Assets/client/css/printCertificate.css" rel="stylesheet" />
    <link href="~/Assets/client/css/dtree.css" rel="stylesheet" />

    <script src="~/Assets/client/js/common/dtree.js"></script>
    <script src="~/Assets/client/js/controllers/printCertificateController.js"></script>
</head>
<body>
    <table class="main" cellpadding="0" cellspacing="0" border="0">
        <tr>
            <td class="menu">
                <a href="https://www.stimulsoft.com/en/products/reports-web" target="_blank"><img alt="Logo" src="~/Assets/client/Images/logoweb.png" style="border: 0;" /></a>
                <div style="height: 2px; border-top: dashed 1px #c8c8c8; padding:3px;"></div>
                <div style="overflow: auto; position: absolute; top: 80px; bottom: 50px; left: 0px; width: 275px;">
                    <script type="text/javascript">printCertificateController.buildReportTree();</script>
                </div>
            </td>
            <td class="line" rowspan="2">
                <div class="line"></div>
            </td>
            <td class="viewer" rowspan="2">
                @RenderBody()
            </td>
        </tr>
        <tr>
            <td class="link">
                <div style="height: 2px; border-top: dashed 1px #c8c8c8; padding:3px;"></div>
                <a href="http://www.stimulsoft.com/" target="_blank"><img class="link" alt="www.stimulsoft.com" border="0" src="~/Assets/client/Images/hyperlink.png" /></a>
            </td>
        </tr>
    </table>
    @RenderSection("footerJS", required: false)
</body>
</html>
