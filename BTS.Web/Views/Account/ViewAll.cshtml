@model IEnumerable<BTS.Web.Models.LicenceViewModel>
@using BTS.Common
@{
    ViewBag.Title = "ViewAll";
    Layout = null;
}

<table class="table table-striped table-condensed MyDataTable" id="MyDataTable">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.machineCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.key)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.isValid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ExpireDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DaysLeft)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.enable)
            </th>
            <th>Thao tác</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.machineCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.key)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.isValid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExpireDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DaysLeft)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.enable)
            </td>
            <td class="dt-body-center">
                <a class="btn btn-info btn-sm" onclick="commonController.Detail('@Url.Action("Detail", "Licence", new { id = @item.Id })')" data data-toggle="tooltip" data-placement="top" title="Chi tiết"><i class="fa fa-address-card fa-lg"></i></a>
            </td>
        </tr>
        }
    </tbody>
</table>