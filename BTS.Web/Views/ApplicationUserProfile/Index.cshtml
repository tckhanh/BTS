@using BTS.Common
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Layouts.cshtml";
}

@section headCSS{
    <link href="~/Assets/client/css/custom.css" rel="stylesheet" />
    <link href="~/Assets/client/css/custom-DataTable.css" rel="stylesheet" />
}

@section footerJS{
